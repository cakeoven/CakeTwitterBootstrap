{
  "name": "cakephp/cakephp_bootstrap",
  "description": "A CakePHP plugin with helpers for Twitter bootstrap",
  "type": "cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "gmponos",
      "email": "gmponos@gmail.com"
    }
  ],
  "keywords": [
    "helper",
    "bootstrap"
  ],
  "homepage": "http://github.com/gmponos/CakeTwitterBootstrap",
  "support": {
    "issues": "https://github.com/gmponos/CakeTwitterBootstrap/issues"
  },
  "autoload": {
    "psr-4": {
      "CakeBootstrap\\": "src"
    }
  },
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "^3.1"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "2.*",
    "phpunit/phpunit": "4.1.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
