{
  "name": "cakeoven/cakebootstrap",
  "description": "A CakePHP plugin with helpers for Twitter bootstrap",
  "homepage": "https://github.com/cakeoven/CakeBootstrap",
  "type": "cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "gmponos",
      "email": "gmponos@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/cakeoven/CakeBootstrap/issues"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.16",
    "composer/installers": "*"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "2.*",
    "phpunit/phpunit": "4.1.*"
  },
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    },
    "installer-name": "CakeBootstrap"
  }
}
